{
  "files.exclude": {
    "**/*.css.map": true,
    "package-lock.json": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/*.css": true,
    "**/*.css.map": true,
    "package.json": true,
    "package-lock.json": true,
    "**/node_modules": true
  },
  "tailwindCSS.includeLanguages": {
    "liquid": "html"
  },
  "files.associations": {
    "*.liquid": "liquid"
  },
  "tailwindCSS.experimental.classRegex": [
    ["class\\s*=\\s*\"([^\"]*)\"", "g"]
  ]
}
