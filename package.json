{
    "name": "telecenter-eletronica",
    "version": "1.0.0",
    "scripts": {
        "dev": "concurrently \"gulp\" \"shopify theme dev --store=telecenter-eletronica\"",
        "prod": "shopify theme push --theme \"telecenter-eletronica\" --allow-live --force",
        "preview": "shopify theme push --unpublished --force",
        "git": "git add . && git pull && git commit -m \"telecenter-eletronica\" && git push",
        "publish": "npm run git && npm run prod"
    },
    "devDependencies": {
        "concurrently": "^9.1.2",
        "gulp": "^5.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^6.0.1",
        "gulp-terser": "^2.1.0",
        "sass": "^1.86.0"
    }
}
