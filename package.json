{
    "name": "telecenter-eletronica",
    "version": "1.0.0",
    "scripts": {
        "dev": "concurrently \"gulp\" \"shopify theme dev --store=telecenter-eletronica\"",
        "prod": "shopify theme push --theme \"telecenter-eletronica\" --allow-live --force",
        "git": "git add . && git pull && git commit -m \"telecenter-eletronica\" && git push",
        "deploy": "npm run git && npm run prod"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.0.17",
        "autoprefixer": "^10.4.21",
        "chokidar-cli": "^3.0.0",
        "concurrently": "^9.1.2",
        "esbuild": "^0.25.1",
        "gulp": "^5.0.0",
        "gulp-changed": "^4.0.3",
        "gulp-clean-css": "^4.3.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^6.0.1",
        "gulp-terser": "^2.1.0",
        "postcss": "^8.5.3",
        "postcss-cli": "^11.0.1",
        "sass": "^1.86.0",
        "tailwindcss": "^4.0.17"
    },
    "dependencies": {
        "@tailwindcss/cli": "^4.0.17"
    }
}
