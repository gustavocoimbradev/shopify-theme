<nav>
    <ul>
        {% for link in linklists.main-menu.links %}
            <li>
                <a href="{{ link.url }}">
                    {{ link.title }}
                </a>
                {% if link.links != blank %}
                    <ul>
                        {% for child_link in link.links %}
                            <li>
                                <a href="{{ child_link.url }}">{{ child_link.title }}</a>
                            </li>
                        {% endfor %}
                    </ul>
                {% endif %}
            </li>
        {% endfor %}
    </ul>
</nav>

